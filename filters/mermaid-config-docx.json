{
  "fontFamily": "PingFang SC, Heiti SC, Microsoft YaHei, SimHei, sans-serif",
  "theme": "default",
  "themeVariables": {
    "fontFamily": "PingFang SC, Heiti SC, Microsoft YaHei, SimHei, sans-serif",
    "fontSize": "12px",
    "primaryColor": "#fff",
    "primaryTextColor": "#333",
    "primaryBorderColor": "#666",
    "lineColor": "#666",
    "secondaryColor": "#f5f5f5",
    "tertiaryColor": "#fafafa"
  },
  "flowchart": {
    "useMaxWidth": true,
    "htmlLabels": true,
    "curve": "basis",
    "padding": 18,
    "nodeSpacing": 50,
    "rankSpacing": 60,
    "wrappingWidth": 280,
    "diagramPadding": 15
  },
  "sequence": {
    "useMaxWidth": true,
    "wrap": true,
    "wrapPadding": 8,
    "boxMargin": 8,
    "boxTextMargin": 5,
    "noteMargin": 8,
    "messageMargin": 30,
    "mirrorActors": false,
    "bottomMarginAdj": 1,
    "actorFontSize": 12,
    "noteFontSize": 11,
    "messageFontSize": 12
  },
  "gantt": {
    "useMaxWidth": true,
    "fontSize": 11,
    "barHeight": 20,
    "barGap": 4,
    "topPadding": 50,
    "leftPadding": 75,
    "gridLineStartPadding": 35,
    "numberSectionStyles": 4,
    "axisFormat": "%Y-%m-%d"
  }
}
