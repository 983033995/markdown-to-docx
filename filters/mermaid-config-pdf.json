{
  "fontFamily": "PingFang SC, Heiti SC, Microsoft YaHei, sans-serif",
  "theme": "default",
  "themeVariables": {
    "fontFamily": "PingFang SC, Heiti SC, Microsoft YaHei, sans-serif",
    "fontSize": "13px",
    "primaryColor": "#fff",
    "primaryTextColor": "#000",
    "primaryBorderColor": "#333",
    "lineColor": "#333",
    "secondaryColor": "#f5f5f5",
    "tertiaryColor": "#fafafa"
  },
  "flowchart": {
    "useMaxWidth": true,
    "htmlLabels": true,
    "curve": "basis",
    "padding": 20,
    "nodeSpacing": 55,
    "rankSpacing": 70,
    "wrappingWidth": 280,
    "diagramPadding": 18
  },
  "sequence": {
    "useMaxWidth": true,
    "wrap": true,
    "wrapPadding": 9,
    "boxMargin": 9,
    "boxTextMargin": 5,
    "noteMargin": 9,
    "messageMargin": 32,
    "mirrorActors": false,
    "bottomMarginAdj": 1,
    "actorFontSize": 13,
    "noteFontSize": 11,
    "messageFontSize": 12
  },
  "gantt": {
    "useMaxWidth": true,
    "fontSize": 11,
    "barHeight": 22,
    "barGap": 5,
    "topPadding": 55,
    "leftPadding": 90,
    "gridLineStartPadding": 38,
    "numberSectionStyles": 4,
    "axisFormat": "%Y-%m-%d"
  }
}
